#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/apps.py:42
msgid "PostgreSQL extensions"
msgstr "PostgreSQL -ын өргөтгөлүүд"

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/fields/array.py:21
#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/forms/array.py:16
#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/forms/array.py:170
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Массивд байгаа %(nth)s буруу байна"

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "Түүвэрлэсэн массив ижил урттай байх ёстой."

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/fields/hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Тэмдэгтийг тэмдэгт/null руу заагч"

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/fields/hstore.py:17
#, fuzzy, python-format
#| msgid "The value of \"%(key)s\" is not a string or null."
msgid "The value of “%(key)s” is not a string or null."
msgstr "\"%(key)s\" -ийн утга тэмдэгт мөр биш эсвэл null биш байна."

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/forms/hstore.py:16
msgid "Could not load JSON data."
msgstr "JSON дата-г уншиж чадахгүй байна."

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "Оролт JSON dictionary байх ёстой."

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:33
msgid "Enter two valid values."
msgstr "Хоёр зөв утга оруулна уу"

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:34
msgid "The start of the range must not exceed the end of the range."
msgstr "Хүрээний эхлэл төгсгөлөөс хэтрэхгүй байх ёстой."

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:82
msgid "Enter two whole numbers."
msgstr "Хоёр бүхэл тоон утга оруулна уу."

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:88
msgid "Enter two numbers."
msgstr "Хоёр тоо оруулна уу."

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:94
msgid "Enter two valid date/times."
msgstr "хоёр зөв огноо/цаг-ыг оруулна уу."

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:100
msgid "Enter two valid dates."
msgstr "Хоёр зөв огноо оруулна уу"

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/validators.py:12
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Жагсаалтанд %(show_value)d зүйл байна, %(limit_value)d -ээс хэтрэхгүй байх "
"ёстой."
msgstr[1] ""
"Жагсаалтанд %(show_value)d зүйлүүд байна, %(limit_value)d -ээс хэтрэхгүй "
"байх ёстой."

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/validators.py:19
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Жагсаалтанд %(show_value)d зүйл байна, %(limit_value)d -ээс ихгүй байх ёстой."
msgstr[1] ""
"Жагсаалтанд %(show_value)d зүйлүүд байна, %(limit_value)d -ээс ихгүй байх "
"ёстой."

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/validators.py:29
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Зарим түлхүүр байхгүй байна: %(keys)s"

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/validators.py:30
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Хэсэг үл мэдэгдэх түлхүүр байна: %(keys)s"

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/validators.py:70
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr ""
"Энэ хүрээ нь %(limit_value)s -тэй бүрэн тэнцүү буюу түүнээс бага байгаа "
"эсэхийг шалгана уу"

#: horizon_env/lib/python3.8/site-packages/django/contrib/postgres/validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr ""
"Энэ хүрээ нь %(limit_value)s -с их буюу тэнцүү байгаа эсэхийг шалгана уу"

#~ msgid "A JSON object"
#~ msgstr "JSON объект"

#~ msgid "Value must be valid JSON."
#~ msgstr "Утга заавал JSON байх ёстой"

#, python-format
#~ msgid "'%(value)s' value must be valid JSON."
#~ msgstr "'%(value)s' JSON байх ёстой."
